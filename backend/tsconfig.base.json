{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@backend/catch-em-all/catch-em-all-controller": [
        "libs/catch-em-all/catch-em-all-controller/src/index.ts"
      ],
      "@backend/catch-em-all/catch-em-all-persistence": [
        "libs/catch-em-all/catch-em-all-persistence/src/index.ts"
      ],
      "@backend/catch-em-all/catch-em-all-provider": [
        "libs/catch-em-all/catch-em-all-provider/src/index.ts"
      ],
      "@backend/memorandum/memorandum-controller": [
        "libs/memorandum/memorandum-controller/src/index.ts"
      ],
      "@backend/memorandum/memorandum-persistence": [
        "libs/memorandum/memorandum-persistence/src/index.ts"
      ],
      "@backend/memorandum/memorandum-provider": [
        "libs/memorandum/memorandum-provider/src/index.ts"
      ],
      "@backend/shared/texts": ["libs/shared/texts/src/index.ts"],
      "@backend/shared/types": ["libs/shared/types/src/index.ts"],
      "@backend/util": ["libs/shared/util/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
